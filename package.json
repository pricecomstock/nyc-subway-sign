{
  "name": "svelte-app",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "validate": "svelte-check",
    "build-dev-server": "tsc -w -p ./server-tsconfig.json",
    "dev-server": "nodemon --watch server-build -r dotenv/config ./server-build/server.js",
    "build-prod-server": "tsc -p ./server-tsconfig.json",
    "prod-server": "NODE_ENV=production node ./server-build/server.js",
    "start-COMMENT": "the below command is used by rollup -c -w",
    "start": "sirv public --host 0.0.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "@types/express": "^4.17.8",
    "@types/node": "^14.11.8",
    "body-parser": "^1.19.0",
    "nodemon": "^2.0.15",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "ts-node": "^10.7.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@types/cors": "^2.8.8",
    "axios": "^0.27.1",
    "cors": "^2.8.5",
    "csv": "^5.3.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "gtfs-realtime-bindings": "0.0.6",
    "sirv-cli": "^1.0.0"
  }
}
